.header{
  height:100rpx;
  width: 100%;
}
.fl1{
  width:45rpx;
  height:45rpx;
  float: left;
  top:5rpx;
}
.input{
  float: left;
  width:76%;
  margin-left:35rpx;
  position: relative;

}
.input image{
  position: absolute;
  width:45rpx;
  height:45rpx;
  left:15rpx;
  
}
.input input{
  height:50rpx;
  background: #f6f6f6;
  font-size:27rpx;
  color:#aaa;
  padding-left:60rpx;
  border-radius:15rpx; 
}
.fl2{
  width:40rpx;
  height:40rpx;
  float:right;
  margin-right:10rpx;
}
.fl{
  position: relative;
  left:10rpx;
}
.fl text{
  font-size:20rpx;
  position: absolute;
  left:0;
  color:#aaa;
  top:37rpx;
}
.list{
  position: relative;
  right:10rpx;
}
.list text{
  font-size:20rpx;
  position: absolute;
  right:10rpx;
  color:#aaa;
  top:37rpx;
  
}
.goodsList{
  margin:0 auto;
  width:700rpx;
}
.goodsList ul li{
  display: block;
  width:48%;
  height:450rpx;
  margin-right:10rpx;
  float: left;
}
.goodsList ul li image{
  width:100%;
  height:300rpx;
}
.goodsList ul li text{
  color:#000;
  font-size: 27rpx;
}
.goodsList ul li .aa>text{
  color:rgb(25, 158, 235);
  font-size: 24rpx;
}
.goodsList ul li .aa>image{
  width:45rpx;
  height:45rpx;
  float:right;
  margin-right:20rpx;
}
.aa{
  margin-top:10px;
}
.sx{
  padding:0 50rpx;
  box-sizing:border-box;
  display: flex;
  justify-content: space-around;
  font-size:27rpx;
}
.sx view{
  position: relative;
}
.sx view image{
  width:30rpx;
  height:30rpx;
  position: absolute;
}
.sx view image:last-child{
  top:15rpx;
  right:-28rpx;
}
.nogood{
  margin-top:200rpx;
  text-align: center;
  font-size:40rpx;
  font-family: "simhei";
}